@using Cities = EternalBAND.Business.Cities
@using EternalBAND.DataAccess;
@inject ApplicationDbContext _context
@{
    ViewData["Title"] = "Anasayfa";
}




<div class="page-content">

<section class="bg-home2" id="home">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-7">
                <div class="mb-4 pb-3 me-lg-5">

                    <h1 class="display-5 fw-semibold mb-3 text-white">YENİ BİR <span class="text-yellow fw-bold">İLAN</span> VER</h1>
                    <p class="lead text-white mb-0">Yeni bir ilan verirken<br/>Kendini en iyi şekilde ifade etmeyi unutma</p>
                </div>
                <form asp-action="Posts" asp-controller="Home">
                    <div class="registration-form">
                        <div class="row g-0">
                            <div class="col-md-3">
                                <div class="filter-search-form mt-3 mt-md-0">
                                    <select class="form-select form-control filter-input-box" data-trigger name="s"  id="choices-single-locationn" aria-label="Default select example" placeholder="NE ARIYORSUN?" >
                                     <option disabled="disabled" selected="" value="0">NE ARIYORSUN</option>
                                          @foreach (var i in _context.PostTypes.Where(n=> n.Active))
                                                                               {
                                                                                   <option value="@i.TypeShort">@i.Type</option>
                                                                               }
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="filter-search-form mt-3 mt-md-0">
                                    <select class="form-select  filter-input-box" data-trigger name="e"  id="choices-single-locationnn" aria-label="Default select example" placeholder="ENSTRÜMAN?">
                                          <option disabled="disabled" selected="" value="0">ENSTRÜMAN</option>
                                        @foreach (var i in _context.Instruments.Where(n=> n.IsActive))
                                        {
                                            <option value="@i.InstrumentShort">@i.Instrument</option>
                                        }
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="filter-search-form mt-3 mt-md-0">
                                    <select class="form-select  filter-input-box" data-trigger name="c"  id="choices-single-locationn" aria-label="Default select example" placeholder="ŞEHİR?">
                                        <option disabled="disabled" selected="">ŞEHİR</option>
                                       @foreach (var city in Cities.GetCities())
                                       {
                                           <option id="@city.Key">@city.Value</option>
                                       }
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="mt-3 mt-md-0 h-100">
                                    <button class="btn  bg-yellow submit-btn w-100 h-100" type="submit"><i class="uil uil-search me-1"></i> ARA</button>
                                </div>
                            </div>
                        </div><!--end row-->
                    </div>
                </form>
            </div>
            <!--end col-->
            <div class="col-lg-5">
                <div class="mt-5 mt-md-0">
                    <img src="/images/main/emre.png" alt="" class="home-img"/>
                </div>
            </div><!--end col-->
        </div>
        <!--end row-->
    </div>
    <!--end container-->
</section>
<!-- End Home -->

