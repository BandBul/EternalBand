@using EternalBAND.Api.Options;
@using EternalBAND.Common;
@using EternalBAND.DomainObjects.ViewModel;
@using Microsoft.AspNetCore.Identity
@using Microsoft.EntityFrameworkCore
@using Microsoft.Extensions.Options
@{
    var input =
        new BaseNotificationModel()
        {
            NotificationType = NotificationType.PostSharing,
            Title = "Bildirimler",
            IconHtmlContext = "<i class=\"mdi mdi-bell fs-22\"></i>",
            NotificationCountIdName = "notificationCountId",
            SignalRBroadCastingTitle = BroadCastingTitle.ReceiveNotification,
            ReadedNotificationCountIdName = "NotificationCountId"
        };
}
<partial name="PartialViews/_WarningPopup" model="@TempData["WarningMessage"]" />
@{
    await Html.RenderPartialAsync("PartialViews/_BaseNotification", input);
}
